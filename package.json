{
    "name": "sicura_api",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev": "DEBUG=app nodemon ./src/app.js --exec babel-node",
        "start": "node ./dist/app.js",
        "build": "npm run clean && babel ./src -d ./dist",
        "migrate": "sequelize db:migrate",
        "migrate:undo:all": "sequelize db:migrate:undo:all",
        "migrate:undo": "sequelize db:migrate:undo",
        "refreshdb": "sequelize db:migrate:undo:all && sequelize db:migrate",
        "refresh:seed": "npm run refreshdb && npm run seed",
        "heroku-postbuild": "npm run build && npm run migrate",
        "seed": "sequelize db:seed:all",
        "seed:test": "sequelize db:seed:all --env=test",
        "clean": "rm -rf src/dist",
        "lint": "eslint **/*.js --fix",
        "pretest": "rm -rf ./src/database_test.sqlite3 && NODE_ENV=test npm run refreshdb",
        "test": "NODE_ENV=test nyc --reporter=html --reporter=text --require @babel/register --env=test mocha --timeout 10000 ./tests/**/*.spec.js --exit",
        "merge:lcov": "cat ./coverage/lcov.*.info > ./coverage/lcov.info",
        "coverage": "nyc report --reporter=text-lcov | coveralls"
    },
    "dependencies": {
        "@babel/core": "^7.7.4",
        "@babel/plugin-transform-runtime": "^7.8.3",
        "@babel/runtime": "^7.8.3",
        "@sendgrid/mail": "^6.4.0",
        "axios": "^0.19.0",
        "babel-core": "^6.26.3",
        "babel-plugin-module-resolver": "^3.2.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-register": "^6.26.0",
        "bcrypt": "^3.0.7",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "cross-env": "^6.0.3",
        "csv-parser": "^2.3.2",
        "csvtojson": "^2.0.10",
        "debug": "^4.1.1",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-session": "^1.17.0",
        "express-validator": "^6.3.0",
        "jsonwebtoken": "^8.5.1",
        "mailgen": "^2.0.10",
        "moment": "^2.29.2",
        "morgan": "^1.9.1",
        "multer": "^1.4.2",
        "node-geocoder": "^3.25.0",
        "passport": "^0.4.0",
        "passport-facebook": "^3.0.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-twitter": "^1.0.4",
        "pg": "^7.14.0",
        "pg-hstore": "^2.3.3",
        "random-string": "^0.2.0",
        "randomstring": "^1.1.5",
        "sequelize": "^5.21.2",
        "sequelize-cli": "^5.5.1",
        "uuid": "^3.4.0",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-eslint": "^10.0.3",
        "chai": "^4.2.0",
        "coveralls": "^3.0.9",
        "eslint": "^6.7.2",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-import-resolver-babel-module": "^5.1.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "istanbul": "^0.4.5",
        "mocha": "^6.2.2",
        "nodemon": "^2.0.1",
        "nyc": "^14.1.1",
        "sinon": "^7.5.0",
        "supertest": "^4.0.2"
    }
}
